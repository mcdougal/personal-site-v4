
@-moz-keyframes fadein {
   0% { opacity: 0; }
   100% { opacity: 1; } }

@-webkit-keyframes fadein {
   0% { opacity: 0; }
   100% { opacity: 1; } }

@keyframes fadein {
   0% { opacity: 0; }
   100% { opacity: 1; } }

button::-moz-focus-inner {
  border: 0;
}

* {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  *behavior: url(boxsizing.htc); }

*::-moz-focus-inner { 
  border: 0; }

.cache_img {
  left: -1000px;
  position: absolute;
  top: -1000px;
}

body {
  font-family: "Open Sans";
  margin: 0;
  padding: 0; }

a {
  outline: 0;
  text-decoration: none; }

.me {
  bottom: -30px;
  position: fixed;
  right: -200px;
  -moz-transition: all .2s ease-in-out;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out;
  width: 200px;
  z-index: 100; }

.head .name:hover .me {
  -webkit-transform: translateX(-170px) rotate(-8deg);
  -moz-transform: translateX(-170px) rotate(-8deg);
  transform: translateX(-170px) rotate(-8deg); }

.head {
  background: #333;
  -moz-box-shadow: 0px 3px 1px #000;
  -webkit-box-shadow: 0px 3px 1px #000;
  box-shadow: 0px 3px 1px #000;
  height: 43px;
  padding-top: 3px;
  position: relative; }

.head .name {
  float: left;
  font-size: 28px;
  line-height: 37px;
  margin-left: 20px;
  text-shadow: 1px 1px #000;
  -moz-transition: all .2s ease-in-out;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out; }

.head .name a {
  color: #fff; }

.head .name a:hover {
  text-decoration: none; }

.head .name:hover {
  text-shadow: -1px -1px #42c0fb; }

.head .name .cedric {
  font-weight: 300; }

.head .name .mcdougal {
  font-weight: 600; }

.head .contact {
  float: right;
  margin-right: 20px;
  position: relative; }

.head .contact .btn {
  background: transparent;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  display: inline-block;
  height: 22px;
  margin: 9px 10px;
  position: relative;
  -moz-transition: all .2s ease-in-out;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out;
  width: 22px; }

.head .contact .btn:hover {
  background: #42c0fb;
  -moz-box-shadow: 0px 0px 3px 1px #42c0fb;
  -webkit-box-shadow: 0px 0px 3px 1px #42c0fb;
  box-shadow: 0px 0px 3px 1px #42c0fb; }

.head .contact .link {
  background-color: transparent;
  background-image: url(../img/media_icons_inverse.png);
  background-size: 22px;
  color: #fff;
  display: inline-block;
  height: 22px;
  width: 22px; }

.head .contact .link.facebook { background-position: 0px 0px; }
.head .contact .link.linkedin { background-position: 0px 22px; }
.head .contact .link.github { background-position: 0px 44px; }
.head .contact .link.twitter { background-position: 0px 66px; }
.head .contact .link.gmail { background-position: 0px 88px; }

.nav {
  background: #e4e4e4;
  padding-bottom: 3px;
  padding-top: 5px;
  text-align: center; }

.nav .link {
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  color: #333;
  cursor: pointer;
  line-height: 44px;
  padding: 1px 12px 3px 12px;
  -moz-transition: all .2s ease-in-out;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out; }

.nav .link:not(:first-child) {
  margin-left: 10px; }

.nav .link:hover {
  text-decoration: none; }

.nav .link:not(.selected):hover {
  background: rgba(255,255,255,.45); }

.nav .link.selected {
  background: #fff;
  color: #000;
  cursor: default; }

.nav .link.selected[href="#music"] {
  background: #222;
  color: #fff; }

.pages {}

.pages .page {
  display: none;
  padding-top: 25px;
  text-align: center; }

.pages .page.selected {
  display: block; }

.pages .page.bio {
  padding-top: 50px; }

.pages .page.music {
  background: #000;
  height: 100%;
  padding: 0px;
  padding-bottom: 20px;
  padding-top: 20px; }

.pages .page.bio a {
  color: #42c0fb;
  -moz-transition: all .2s ease-in-out;
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out; }

.pages .page.bio a:hover {
  color: #059ce3; }

.pages .page.bio .text {
  color: #666;
  display: block;
  font-size: 18px;
  margin: 0 auto;
  margin-top: 40px;
  max-width: 800px;
  text-align: justify; }

.pages .page.bio .text.hello {
  text-align: center; }

.pages .page.bio .name {
  display: block;
  font-family: 'Crete Round', serif;
  font-size: 80px;
  margin-top: 20px; }

.pages .page .tile {
  display: inline-block;
  margin: 20px 30px;
  padding: 10px;
  vertical-align: top;
  width: 360px; }

.pages .page .tile:hover {
  text-decoration: none; }

.pages .page .tile .image {
  -moz-box-shadow: 0px 0px 3px #000;
  -webkit-box-shadow: 0px 0px 3px #000;
  box-shadow: 0px 0px 3px #000;
  height: 270px;
  -moz-transition: opacity .2s ease-in-out;
  -webkit-transition: opacity .2s ease-in-out;
  transition: opacity .2s ease-in-out;
  width: 360px; }

.pages .page:not(.music) .tile:hover .image {
  opacity: .75; }

.pages .page .tile .title {
  color: #000;
  display: block;
  font-size: 15px;
  margin-top: 18px;
  -moz-transition: opacity .2s ease-in-out;
  -webkit-transition: opacity .2s ease-in-out;
  transition: opacity .2s ease-in-out; }

.pages .page.music .tile .title {
  color: #fff;
  margin-bottom: 10px;
  margin-top: 0; }

.pages .page:not(.music) .tile:hover .title {
  opacity: .6; }

.music-player.ff {
    display: none;
}

@-moz-document url-prefix() {
    .music-player {
        display: none;
    }
    .music-player.ff {
        display: inline-block;
    }
}



.description {
  display: none; }




.fadein {
  -moz-animation-duration: .5s;
  -moz-animation-fill-mode: forwards;
  -moz-animation-name: fadein;
  -moz-animation-timing-function: ease-in-out;
  -webkit-animation-duration: .5s;
  -webkit-animation-fill-mode: forwards;
  -webkit-animation-name: fadein;
  -webkit-animation-timing-function: ease-in-out;
  animation-duration: .5s;
  animation-fill-mode: forwards;
  animation-name: fadein;
  animation-timing-function: ease-in-out;
  opacity: 0; }

.foldin {
  -moz-animation-duration: .5s;
  -moz-animation-fill-mode: forwards;
  -moz-animation-name: fadein;
  -moz-animation-timing-function: ease-in-out;
  -webkit-animation-duration: .5s;
  -webkit-animation-fill-mode: forwards;
  -webkit-animation-name: fadein;
  -webkit-animation-timing-function: ease-in-out;
  animation-duration: .5s;
  animation-fill-mode: forwards;
  animation-name: fadein;
  animation-timing-function: ease-in-out;
  opacity: 0; }

